p#notice = notice
h1 #{@entry.name}


h2 Emails: 
- @entry.emails.each do |em|
  ul
    li = link_to em.name, em.to_email

h2 Websites: 
- @entry.weburls.each do |w|
  ul
    li = link_to w.name, w.to_url
    
h2 Addresses:
- @entry.addresses.each do |address|
  ul
    li
      strong = address.address_type.name
      ':  
      = address.street.titleize
      ',
      = address.city
      ',
      = address.state
      ' 
      = address.zip

table
  tr
    td
      = link_to 'Edit', edit_entry_path(@entry)
    td
      = button_to 'Destroy', @entry, :confirm => 'Are you sure?', :method => :delete
    td
      = link_to 'Back', entries_path
